			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:totalDigits value="15"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="НалБазаДоляПнСт" use="optional">
			<xs:annotation>
				<xs:documentation>строка 051</xs:documentation>
				<xs:documentation>Налоговая база исходя из доли, в том числе для исчисления налога по пониженной ставке</xs:documentation>
			</xs:annotation>
