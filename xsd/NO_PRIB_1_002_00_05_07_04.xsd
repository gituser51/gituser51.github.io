				<xs:restriction base="xs:integer">
					<xs:totalDigits value="15"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ДоляНалБаз" use="optional">
			<xs:annotation>
				<xs:documentation>строка 040</xs:documentation>
				<xs:documentation>Доля налоговой базы (%)</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:decimal">
					<xs:totalDigits value="18"/>
					<xs:fractionDigits value="15"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="НалБазаДоля" use="optional">
			<xs:annotation>
				<xs:documentation>строка 050</xs:documentation>
