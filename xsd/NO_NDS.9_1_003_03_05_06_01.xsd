	<xs:element name="Файл">
		<xs:annotation>
			<!---**************************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Проверка имени файла на соответствие значению атрибута @ИдФайл">
					<sch:rule context="Файл">
						<!--  1. Проверка имени файла на соответствие ИдФайл                                                                             -->
						<sch:assert test="usch:getFileName() = @ИдФайл">
							<usch:error code="0400400007" path="@ИдФайл" value-of-select="@ИдФайл" doc-id-path="usch:getFileName()" doc-id-caption="Файл">Имя файла обмена "<usch:value-of select="usch:getFileName()"/>" не совпадает со значением элемента ИдФайл "<usch:value-of select="@ИдФайл"/>"</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---**************************************************************************************************************************************************************************-->
			<xs:documentation>Файл обмена</xs:documentation>
			<xs:documentation/>
			<xs:documentation>part</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Документ">
					<xs:annotation>
						<!---*********************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Присутствие признака актуальности сведений">
								<sch:rule context="Документ">
									<!-- 2. Контроль  обязательности  присутствия  ПризнСвед9  в  зависимости  от  значения  НомКорр  -->
									<sch:assert test="usch:iif(@НомКорр=0, count(@ПризнСвед9)=0, count(@ПризнСвед9)!=0)">
										<usch:error code="0400300003" path="@ПризнСвед9" value-of-select="@НомКорр">
											Если "Номер корректировки" = 0, то строка 001 должна быть пустой. В противном случае строка 001 должна быть заполнена
											</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*********************************************************************************************************************************************************-->
						<!---*********************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Присутствие КнигаПрод в  зависимости  от  значения ПризнСвед9">
								<sch:rule context="Документ">
									<!-- 3. Контроль  обязательности  присутствия  КнигаПрод в  зависимости  от  значения ПризнСвед9  -->
									<sch:assert test="(count(@ПризнСвед9)!=0 and @ПризнСвед9=0 and count(КнигаПрод)!=0) or (count(@ПризнСвед9)!=0 and @ПризнСвед9=1 and count(КнигаПрод)=0) or (count(@ПризнСвед9)=0 and (count(КнигаПрод)=0 or count(КнигаПрод)!=0))">
										<usch:error code="0400300003" path="КнигаПрод" value-of-select="count(КнигаПрод)">
											Должно выполняться одно из следующих условий: <br/>
											1. "Строка 001" = "0" и Лист 3 присутствует <br/>
											2. "Строка 001" = "1" и Лист 3 отсутствует <br/>
											3. "Строка 001" не заполнена <br/>
										</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*********************************************************************************************************************************************************-->
						<xs:documentation>Титульный лист</xs:documentation>
