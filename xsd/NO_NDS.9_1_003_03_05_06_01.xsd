					<xs:complexType>
						<xs:sequence>
							<xs:element name="КнигаПрод" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Лист 03</xs:documentation>
									<xs:documentation>Сведения из книги продаж об операциях, отражаемых за истекший налоговый период</xs:documentation>
									<xs:documentation>part</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="КнПродСтр" maxOccurs="unbounded">
											<xs:annotation>
												<!---**********************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие сведений о представителе">
														<sch:rule context="КнПродСтр">
															<!-- 4. Контроль  обязательности  присутствия  НомИспрСчФ в  зависимости  от наличия  ДатаИспрСчФ  -->
															<sch:assert test="usch:iif(count(@ДатаИспрСчФ)!=0, count(@НомИспрСчФ)!=0, (count(@НомИспрСчФ)!=0 or count(@НомИспрСчФ)=0))">
																<usch:error code="0400300001" path="@НомИспрСчФ" value-of-select="count(@НомИспрСчФ)">
																	Если заполнена строка 050, то должна быть заполнена строка 040
																</usch:error>
															</sch:assert>
															<!-- 5. Контроль  обязательности  присутствия  ДатаИспрСчФ в  зависимости  от наличия  НомИспрСчФ  -->
															<sch:assert test="usch:iif(count(@НомИспрСчФ)!=0, count(@ДатаИспрСчФ)!=0, (count(@ДатаИспрСчФ)!=0 or count(@ДатаИспрСчФ)=0))">
																<usch:error code="0400300001" path="@ДатаИспрСчФ" value-of-select="count(@ДатаИспрСчФ)">
																	Если заполнена строка 040, то должна быть заполнена строка 050
																</usch:error>
															</sch:assert>
															<!-- 6. Контроль  обязательности  присутствия  НомКСчФПрод в  зависимости  от наличия  ДатаКСчФПрод или НомИспрКСчФ или ДатаИспрКСчФ -->
															<sch:assert test="usch:iif( count(@ДатаКСчФПрод)!=0 or count(@НомИспрКСчФ)!=0 or count(@ДатаИспрКСчФ)!=0 , count(@НомКСчФПрод)!=0, (count(@НомКСчФПрод)!=0 or count(@НомКСчФПрод)=0))">
																<usch:error code="0400300001" path="@НомКСчФПрод" value-of-select="count(@НомКСчФПрод)">
																	Если заполнена хотя бы одна из строк 070 или 080 или 090, то должна быть заполнена строка 060
																</usch:error>
															</sch:assert>
															<!-- 7. Контроль  обязательности  присутствия  ДатаКСчФПрод в  зависимости  от наличия  НомКСчФПрод или НомИспрКСчФ или ДатаИспрКСчФ -->
															<sch:assert test="usch:iif(count(@НомКСчФПрод)!=0 or count(@НомИспрКСчФ)!=0 or count(@ДатаИспрКСчФ)!=0 , count(@ДатаКСчФПрод)!=0, (count(@ДатаКСчФПрод)!=0 or count(@ДатаКСчФПрод)=0))">
																<usch:error code="0400300001" path="@ДатаКСчФПрод" value-of-select="count(@ДатаКСчФПрод)">
																	Если заполнена хотя бы одна из строк 060 или 080 или 090, то должна быть заполнена строка 070
																</usch:error>
															</sch:assert>
															<!-- 8. Контроль  обязательности  присутствия  НомИспрКСчФ в  зависимости  от наличия  ДатаИспрКСчФ  -->
															<sch:assert test="usch:iif(count(@ДатаИспрКСчФ)!=0, count(@НомИспрКСчФ)!=0, (count(@НомИспрКСчФ)!=0 or count(@НомИспрКСчФ)=0))">
																<usch:error code="0400300001" path="@НомИспрКСчФ" value-of-select="count(@НомИспрКСчФ)">
																	Если заполнена строка 090, то должна быть заполнена строка 080
																</usch:error>
															</sch:assert>
															<!-- 9. Контроль  обязательности  присутствия  ДатаИспрКСчФ в  зависимости  от наличия  НомИспрКСчФ  -->
															<sch:assert test="usch:iif(count(@НомИспрКСчФ)!=0, count(@ДатаИспрКСчФ)!=0, (count(@ДатаИспрКСчФ)!=0 or count(@ДатаИспрКСчФ)=0))">
																<usch:error code="0400300001" path="@ДатаИспрКСчФ" value-of-select="count(@ДатаИспрКСчФ)">
																	Если заполнена строка 080, то должна быть заполнена строка 090
																</usch:error>
															</sch:assert>
															<!-- 10. Контроль  обязательности  присутствия  СтоимПродСФВ в  зависимости  от наличия и значения  ОКВ -->
															<sch:assert test="usch:iif(count(@ОКВ)!=0 and @ОКВ!=643 , count(@СтоимПродСФВ)!=0, (count(@СтоимПродСФВ)!=0 or count(@СтоимПродСФВ)=0))">
																<usch:error code="0400300001" path="@СтоимПродСФВ" value-of-select="count(@ОКВ)">
																	Если строка 140 заполнена и не равна 643, то строка 150 должна быть заполнена
																</usch:error>
															</sch:assert>
															<!-- 11. Контроль  обязательности  присутствия  СтоимПродСФ в  зависимости  от наличия  СтоимПродОсв  -->
															<sch:assert test="usch:iif(count(@СтоимПродОсв)=0, count(@СтоимПродСФ)!=0, (count(@СтоимПродСФ)!=0 or count(@СтоимПродСФ)=0))">
																<usch:error code="0400300001" path="@СтоимПродСФ" value-of-select="count(@СтоимПродОсв)">
																	Если строка 220 не заполнена, то строка 160 должна быть заполнена
																</usch:error>
															</sch:assert>
															<!-- 12. Контроль  обязательности  присутствия  СтоимПродОсв в  зависимости  от наличия  СтоимПродСФ  -->
															<sch:assert test="usch:iif(count(@СтоимПродСФ)=0, count(@СтоимПродОсв)!=0, (count(@СтоимПродОсв)!=0 or count(@СтоимПродОсв)=0))">
																<usch:error code="0400300001" path="@СтоимПродОсв" value-of-select="count(@СтоимПродСФ)">
																	Если строка 160 не заполнена, то строка 220 должна быть заполнена
																</usch:error>
															</sch:assert>
															<!-- 13. Контроль  обязательности  присутствия  СумНДССФ20 в  зависимости  от наличия  СтоимПродСФ20  -->
															<sch:assert test="usch:iif(count(@СтоимПродСФ20)!=0, count(@СумНДССФ20)!=0, (count(@СумНДССФ20)!=0 or count(@СумНДССФ20)=0))">
																<usch:error code="0400300001" path="@СумНДССФ20" value-of-select="count(@СтоимПродСФ20)">
																	Если строка 170 заполнена, то строка 200 должна быть заполнена
																</usch:error>
															</sch:assert>
															<!-- 14. Контроль  обязательности  присутствия  СумНДССФ18 в  зависимости  от наличия  СтоимПродСФ18  -->
															<sch:assert test="usch:iif(count(@СтоимПродСФ18)!=0, count(@СумНДССФ18)!=0, (count(@СумНДССФ18)!=0 or count(@СумНДССФ18)=0))">
																<usch:error code="0400300001" path="@СумНДССФ18" value-of-select="count(@СтоимПродСФ18)">
																	Если строка 175 заполнена, то строка 205 должна быть заполнена
																</usch:error>
															</sch:assert>
															<!-- 15. Контроль  обязательности  присутствия  СумНДССФ10 в  зависимости  от наличия  СтоимПродСФ10  -->
															<sch:assert test="usch:iif(count(@СтоимПродСФ10)!=0, count(@СумНДССФ10)!=0, (count(@СумНДССФ10)!=0 or count(@СумНДССФ10)=0))">
																<usch:error code="0400300001" path="@СумНДССФ10" value-of-select="count(@СтоимПродСФ10)">
																	Если строка 180 заполнена, то строка 210 должна быть заполнена
																</usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---**********************************************************************************************************************************************-->
												<xs:documentation>Лист 02</xs:documentation>
