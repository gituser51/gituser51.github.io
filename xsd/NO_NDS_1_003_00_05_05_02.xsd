	<xs:element name="Файл">
		<xs:annotation>
			<!---**************************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Проверка имени файла на соответствие значению атрибута @ИдФайл">
					<sch:rule context="Файл">
						<!-- 1. Проверка имени файла на соответствие ИдФайл -->
						<sch:assert test="usch:getFileName() = @ИдФайл">
							<usch:error code="0400400007" path="@ИдФайл" value-of-select="@ИдФайл" doc-id-path="usch:getFileName()" doc-id-caption="Файл">Имя файла обмена "<usch:value-of select="usch:getFileName()"/>" не совпадает со значением элемента ИдФайл "<usch:value-of select="@ИдФайл"/>"</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---**************************************************************************************************************************************************************************-->
			<!---*************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Присутствие элементов ПризнНал8, ПризнНал81, ПризнНал9, ПризнНал91, ПризнНал10, ПризнНал11, ПризнНал12 в зависимости от значения элемента ПризнНал8-12">
					<sch:rule context="Файл">
						<!-- 2. Контроль  присутствия элементов ПризнНал8, ПризнНал81, ПризнНал9, ПризнНал91, ПризнНал10, ПризнНал11, ПризнНал12 в зависимости от значения элемента ПризнНал8-12 -->
						<sch:assert test="usch:iif(   @ПризнНал8-12=1, 
	(count(@ПризнНал8)!=0 and count(@ПризнНал81)!=0 and count(@ПризнНал9)!=0 and count(@ПризнНал91)!=0 and count(@ПризнНал10)!=0 and count(@ПризнНал11)!=0 and count(@ПризнНал12)!=0) , 
	(count(@ПризнНал8)=0 and count(@ПризнНал81)=0 and count(@ПризнНал9)=0 and count(@ПризнНал91)=0 and count(@ПризнНал10)=0 and count(@ПризнНал11)=0 and count(@ПризнНал12)=0)  )">
							<usch:error code="0400300003" path="@ПризнНал8-12" value-of-select="@ПризнНал8-12"> Атрибуты Файл/(ПризнНал8, ПризнНал81, ПризнНал9, ПризнНал91, ПризнНал10, ПризнНал11, ПризнНал12) должны присутствовать при Файл/ПризнНал8-12 равном 1. В противном случае данные атрибуты должны отсутствовать</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*************************************************************************************************************************************************************-->
			<!---*************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Значение ПризнНал8 в зависимости от значений других элементов">
					<sch:rule context="Файл">
						<!-- 3. Контроль значения элемента ПризнНал8 в  зависимости  от  значений других элементов -->
						<sch:assert test="usch:iif (
 (
 count(Документ/НДС/КнигаПокупДЛ)!=0 or 
( count(Документ/НДС/СумУпл164/СумНалВыч/@НалВычОбщ)!=0  and  Документ/НДС/СумУпл164/СумНалВыч/@НалВычОбщ>0 ) or 
( count(Документ/НДС/НалПодтв0/СумОпер4/@НалВычПод)!=0  and  Документ/НДС/НалПодтв0/СумОпер4/@НалВычПод>0 ) or 
( count(Документ/НДС/НалПодтв0/СумОпер4/@НалНеПод)!=0  and  Документ/НДС/НалПодтв0/СумОпер4/@НалНеПод>0 ) or 
( count(Документ/НДС/НалВычПред0/СумПер/СумОпер5/@НалВычПод)!=0  and  Документ/НДС/НалВычПред0/СумПер/СумОпер5/@НалВычПод>0 ) or 
( count(Документ/НДС/НалВычПред0/СумПер/СумОпер5/@НалВычНеПод)!=0  and  Документ/НДС/НалВычПред0/СумПер/СумОпер5/@НалВычНеПод>0 ) or 
( count(Документ/НДС/НалНеПодтв0/СумОпер6/@НалВычНеПод)!=0  and  Документ/НДС/НалНеПодтв0/СумОпер6/@НалВычНеПод>0 ) or 
( count(Документ/НДС/НалНеПодтв0/КорНалИзмЦен/@КорНалБазаУм)!=0  and  Документ/НДС/НалНеПодтв0/КорНалИзмЦен/@КорНалБазаУм>0 )
 ) ,
(count(@ПризнНал8)!=0  and  @ПризнНал8!=0) ,
(count(@ПризнНал8)=0  or count(@ПризнНал8)!=0)  
 )">
							<usch:error code="0400400007" path="@ПризнНал8" value-of-select="@ПризнНал8"> При выполнении хотя бы одного из нижеперечисленных условий Раздел 8 должен присутствовать: <br/>
								1. Раздел 3 строка 190 > 0 <br/>
								2. Раздел 4 строка 030 > 0 <br/>
								3. Раздел 4 строка 040 > 0 <br/>
								4. Раздел 5 строка 050 > 0 <br/>
								5. Раздел 5 строка 070 > 0 <br/>
								6. Раздел 6 строка 040 > 0 <br/>
								6. Раздел 6 строка 140 > 0 <br/>
								7. Присутствует дополнительный лист из Книги покупок								
							</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*************************************************************************************************************************************************************-->
			<!---*************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Значение ПризнНал9 в зависимости от значений других элементов">
					<sch:rule context="Файл">
						<!-- 4. Контроль значения элемента ПризнНал9 в  зависимости  от  значений других элементов -->
						<sch:assert test="usch:iif (
 ( 
 count(Документ/НДС/КнигаПродДЛ)!=0 or
( count(Документ/НДС/СумУпл164/СумНалОб/@НалВосстОбщ)!=0  and  Документ/НДС/СумУпл164/СумНалОб/@НалВосстОбщ>0 ) or
( count(Документ/НДС/НалПодтв0/СумОпер4/@НалВосст)!=0  and  Документ/НДС/НалПодтв0/СумОпер4/@НалВосст>0 ) or
( count(Документ/НДС/НалПодтв0/КорНБВозврТов/@НалВосст)!=0  and  Документ/НДС/НалПодтв0/КорНБВозврТов/@НалВосст>0 )  or
( count(Документ/НДС/НалНеПодтв0/СумОпер6/@СумНал164)!=0  and  Документ/НДС/НалНеПодтв0/СумОпер6/@СумНал164>0 )  or
( count(Документ/НДС/НалНеПодтв0/КорНалВозврТов/@НалВосст)!=0  and  Документ/НДС/НалНеПодтв0/КорНалВозврТов/@НалВосст>0 )  or
( count(Документ/НДС/НалНеПодтв0/КорНалИзмЦен/@КорНалБазаУв)!=0  and  Документ/НДС/НалНеПодтв0/КорНалИзмЦен/@КорНалБазаУв>0 )
 ) ,
count(@ПризнНал9)!=0  and  @ПризнНал9!=0 ,
( count(@ПризнНал9)=0  or  count(@ПризнНал9)!=0) 
 )">
							<usch:error code="0400400007" path="@ПризнНал9" value-of-select="@ПризнНал9"> При выполнении хотя бы одного из нижеперечисленных условий Раздел 9 должен присутствовать: <br/>
								1. Раздел 3 строка 118 > 0 <br/>
								2. Раздел 4 строка 050 > 0 <br/>
								3. Раздел 4 строка 080 > 0 <br/>
								4. Раздел 6 строка 130 > 0 <br/>
								5. Раздел 6 строка 100 > 0 <br/>
								6. Раздел 6 строка 120 > 0 <br/>								
								7. Присутствует дополнительный лист из Книги продаж	
							</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*************************************************************************************************************************************************************-->
			<!---*************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Значение ПризнНал12 в зависимости от значений других элементов">
					<sch:rule context="Файл">
						<!-- 5. Контроль значения элемента ПризнНал12 в  зависимости  от  значений других элементов -->
						<sch:assert test="usch:iif( ( count(Документ/НДС/СумУплНП/@СумПУ_173.5)!=0  and  Документ/НДС/СумУплНП/@СумПУ_173.5>0 ) ,
(count(@ПризнНал12)!=0  and  @ПризнНал12!=0) , ( count(@ПризнНал12)=0  or  @ПризнНал12=0) )">
							<usch:error code="0400400007" path="@ПризнНал12" value-of-select="@ПризнНал12"> Если Раздел 1 строка 030 > 0, то Раздел 12 должен присутствовать </usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*************************************************************************************************************************************************************-->
			<!---*************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Присутствие элемента КнигаПокуп в зависимости от значения элемента ПризнНал8">
					<sch:rule context="Файл">
						<!-- 6. Контроль  присутствия элемента КнигаПокуп в зависимости от значения элемента ПризнНал8 -->
						<sch:assert test="usch:iif(   @ПризнНал8=1, 	count(Документ/НДС/КнигаПокуп)!=0 , 	(count(Документ/НДС/КнигаПокуп)=0  or  count(Документ/НДС/КнигаПокуп)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/КнигаПокуп" value-of-select="count(Документ/НДС/КнигаПокуп)">  Если Раздел 8 присутствует, то должно быть задано имя файла Книги покупок Файл/Документ/НДС/КнигаПокуп </usch:error>
						</sch:assert>
						<!-- 7. Контроль  присутствия элемента КнигаПокупДЛ в зависимости от значения элемента ПризнНал81 -->
						<sch:assert test="usch:iif(   @ПризнНал81=1, 	count(Документ/НДС/КнигаПокупДЛ)!=0 , 	(count(Документ/НДС/КнигаПокупДЛ)=0  or  count(Документ/НДС/КнигаПокупДЛ)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/КнигаПокупДЛ" value-of-select="count(Документ/НДС/КнигаПокупДЛ)">   Если Раздел 8.1 присутствует, то должно быть задано имя файла Дополнительного листа книги покупок Файл/Документ/НДС/КнигаПокупДЛ </usch:error>
						</sch:assert>
						<!-- 8. Контроль  присутствия элемента КнигаПрод в зависимости от значения элемента ПризнНал9 -->
						<sch:assert test="usch:iif(   @ПризнНал9=1, 	count(Документ/НДС/КнигаПрод)!=0 , 	(count(Документ/НДС/КнигаПрод)=0  or  count(Документ/НДС/КнигаПрод)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/КнигаПрод" value-of-select="count(Документ/НДС/КнигаПрод)">   Если Раздел 9 присутствует, то должно быть задано имя файла Книги продаж Файл/Документ/НДС/КнигаПрод </usch:error>
						</sch:assert>
						<!-- 9. Контроль  присутствия элемента КнигаПродДЛ в зависимости от значения элемента ПризнНал91 -->
						<sch:assert test="usch:iif(   @ПризнНал91=1, 	count(Документ/НДС/КнигаПродДЛ)!=0 , 	(count(Документ/НДС/КнигаПродДЛ)=0  or  count(Документ/НДС/КнигаПродДЛ)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/КнигаПродДЛ" value-of-select="count(Документ/НДС/КнигаПродДЛ)">  Если Раздел 9.1 присутствует, то должно быть задано имя файла Дополнительного листа книги покупок Файл/Документ/НДС/КнигаПродДЛ </usch:error>
						</sch:assert>
						<!-- 10. Контроль  присутствия элемента ЖУчВыстСчФ в зависимости от значения элемента ПризнНал10 -->
						<sch:assert test="usch:iif(   @ПризнНал10=1, 	count(Документ/НДС/ЖУчВыстСчФ)!=0 , 	(count(Документ/НДС/ЖУчВыстСчФ)=0  or  count(Документ/НДС/ЖУчВыстСчФ)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/ЖУчВыстСчФ" value-of-select="count(Документ/НДС/ЖУчВыстСчФ)"> Если Раздел 10 присутствует, то должно быть задано имя файла для него Файл/Документ/НДС/ЖУчВыстСчФ </usch:error>
						</sch:assert>
						<!-- 11. Контроль  присутствия элемента ЖУчПолучСчФ в зависимости от значения элемента ПризнНал11 -->
						<sch:assert test="usch:iif(   @ПризнНал11=1, 	count(Документ/НДС/ЖУчПолучСчФ)!=0 , 	(count(Документ/НДС/ЖУчПолучСчФ)=0  or  count(Документ/НДС/ЖУчПолучСчФ)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/ЖУчПолучСчФ" value-of-select="count(Документ/НДС/ЖУчПолучСчФ)"> Если Раздел 11 присутствует, то должно быть задано имя файла для него Файл/Документ/НДС/ЖУчПолучСчФ </usch:error>
						</sch:assert>
						<!-- 12. Контроль  присутствия элемента ВыстСчФ_173.5 в зависимости от значения элемента ПризнНал12 -->
						<sch:assert test="usch:iif(   @ПризнНал12=1, 	count(Документ/НДС/ВыстСчФ_173.5)!=0 , 	(count(Документ/НДС/ВыстСчФ_173.5)=0  or  count(Документ/НДС/ВыстСчФ_173.5)!=0)  )">
							<usch:error code="0400300003" path="Документ/НДС/ВыстСчФ_173.5" value-of-select="count(Документ/НДС/ВыстСчФ_173.5)"> Если Раздел 12 присутствует, то должно быть задано имя файла для него Файл/Документ/НДС/ВыстСчФ_173.5 </usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*************************************************************************************************************************************************************-->
			<xs:documentation>Файл обмена</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Документ">
					<xs:annotation>
						<!---*************************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Присутствие ФИО (руководителя организации / представителя)  в  зависимости  от  значения  ПрПодп и наличия НПЮЛ">
								<sch:rule context="Документ">
									<!-- 13. Контроль  обязательности  присутствия  ФИО (руководителя организации / представителя)  в  зависимости  от  значения  ПрПодп и наличия НПЮЛ -->
									<sch:assert test=" (Подписант/ФИО and Подписант/@ПрПодп=2) or (Подписант/ФИО and Подписант/@ПрПодп=1 and СвНП/НПЮЛ) or 		( ( (Подписант/ФИО) or  not(Подписант/ФИО) )  and Подписант/@ПрПодп=1 and СвНП/НПФЛ) ">
										<usch:error code="0400300003" path="Подписант/ФИО" value-of-select="Подписант/@ПрПодп">
										<usch:value-of select="Подписант/@ПрПодп"/>
											Должно выполняться одно из условий: <br/>
											1. Указано ФИО подписанта и налогоплательщик является представителем <br/>
											2. Указано ФИО подписанта, налогоплательщик не является представителем и налогоплательщик является юридическим лицом <br/>
											3. Налогоплательщик не является представителем и является физическим лицом
										</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*************************************************************************************************************************************************************-->
						<!---*************************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Значение ПоМесту в зависимости от значения КПП">
								<sch:rule context="Документ">
									<!-- 14. Контроль значения элемента ПоМесту в  зависимости  от  значения элемента  КПП -->
									<sch:assert test="
( count(СвНП/НПЮЛ/@КПП)!=0 and 
( ( (substring(СвНП/НПЮЛ/@КПП,5,2)=50) and @ПоМесту=213 ) 
or 
( ( (substring(СвНП/НПЮЛ/@КПП,5,2)=36) or (substring(СвНП/НПЮЛ/@КПП,5,2)=76) or (substring(СвНП/НПЮЛ/@КПП,5,2)=51) ) and @ПоМесту=250 ) 
or 
( ( (number(substring(СвНП/НПЮЛ/@КПП,5,2)='4T') ) or (number(substring(СвНП/НПЮЛ/@КПП,5,2)='5G') ) ) and @ПоМесту=227 ) 
or 
( (substring(СвНП/НПЮЛ/@КПП,5,2)!=76) and (substring(СвНП/НПЮЛ/@КПП,5,2)!=36) and (number(substring(СвНП/НПЮЛ/@КПП,5,2)!='4T') ) and (number(substring(СвНП/НПЮЛ/@КПП,5,2)!='5G') ) and (@ПоМесту=214 or @ПоМесту=215 or @ПоМесту=216 or @ПоМесту=231  or @ПоМесту=331) ) ) ) 
or 
( count(СвНП/НПЮЛ/@КПП)=0 )
">
										<usch:error code="0400400007" path="@ПоМесту" value-of-select="@ПоМесту"> 
											Должно выполняться одно из условий: <br/>
											1. Если 5 и 6 знаки КПП = "50", то поле По месту = "213" <br/>
											2. Если 5 и 6 знаки КПП = "36" или "76" или "51", то поле По месту = "250"<br/>
											3. Если 5 и 6 знаки КПП = "4T" или "5G", то поле По месту = "227"<br/>
											4. Если 5 и 6 знаки КПП не являются значениями "36", "76", "4T", "5G", то поле По месту должно быть одним из значений: 214, 215, 216, 231, 331<br/>						
										</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*************************************************************************************************************************************************************-->
						<!---*************************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Присутствие элемента СумУплНА в зависимости от значения элемента ПоМесту">
								<sch:rule context="Документ">
									<!-- 15. Контроль  обязательности  присутствия элемента СумУплНА в зависимости от значения элемента ПоМесту -->
									<sch:assert test="usch:iif(   @ПоМесту=231, count(НДС/СумУплНА)!=0, (count(НДС/СумУплНА)!=0 or count(НДС/СумУплНА)=0)  )">
										<usch:error code="0400300003" path="НДС/СумУплНА" value-of-select="count(НДС/СумУплНА)"> Если значение поля "По месту" = "231", то Раздел 2 должен присутствовать </usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*************************************************************************************************************************************************************-->
						<!---*************************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Значение элемента Период в зависимости от значения элемента ПоМесту">
								<sch:rule context="Документ">
									<!-- 16. Контроль значения элемента Период в  зависимости  от  значения элемента  ПоМесту -->
									<sch:assert test="usch:iif( (@Период=01 or  @Период=02 or @Период=03 or @Период=04 or @Период=05 or @Период=06 or @Период=07 or @Период=08 or @Период=09 or @Период=10 or @Период=11 or @Период=12) , @ПоМесту=250 , @ПоМесту=250 or @ПоМесту!=250 )">
										<usch:error code="0400400007" path="@Период" value-of-select="@Период">  Если значение поля "Период" принимает одно из значений: 01, 02, 03, 04, 05, 06, 07, 08, 09, 10, 11, 12, то значение поля "По месту" должно быть равно 250</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*************************************************************************************************************************************************************-->
						<!---*************************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Присутствие элемента СумВосУпл в зависимости от значения элемента Период">
								<sch:rule context="Документ">
									<!-- 17. Контроль  обязательности  присутствия элемента СумВосУпл в зависимости от значения элемента Период -->
									<sch:assert test="usch:iif(   (@Период=24 or @Период=56) , count(НДС/СумУпл164/СумВосУпл)!=0  or  count(НДС/СумУпл164/СумВосУпл)=0, count(НДС/СумУпл164/СумВосУпл)=0  )">
										<usch:error code="0400300003" path="НДС/СумУпл164/СумВосУпл" value-of-select="count(НДС/СумУпл164/СумВосУпл)"> Если значение поля "Период" равно 24 или 56, то Приложение 1 к Разделу 3 должно отсутствовать</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*************************************************************************************************************************************************************-->
						<xs:documentation>Титульный лист</xs:documentation>
						<xs:documentation>Состав и структура документа</xs:documentation>
						<xs:documentation>part</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвНП">
								<xs:annotation>
									<xs:documentation/>
									<xs:documentation>Сведения о налогоплательщике</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="НПЮЛ">
											<xs:annotation>
												<xs:documentation/>
												<xs:documentation>Налогоплательщик - юридическое лицо</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="СвРеоргЮЛ" minOccurs="0">
														<xs:annotation>
															<!---*********************************************************************************************************************************-->
															<xs:appinfo>
																<sch:pattern name="Присутствие атрибутов ИННЮЛ и КПП">
																	<sch:rule context="СвРеоргЮЛ">
																		<!-- 18. Контроль  обязательности  присутствия  атрибутов  ИННЮЛ  и  КПП  в  зависимости  от  значения  атрибута  ФормРеорг       -->
																		<sch:assert test=" (count(@ИННЮЛ)!=0 and @ФормРеорг!=0) or (count(@ИННЮЛ)=0 and @ФормРеорг=0)  ">
																			<usch:error code="0400300003" path="@ИННЮЛ" value-of-select="@ФормРеорг"> Если заполнено поле "ИНН реорганизованной организации", то поле "Форма реорганизации" не должно быть равно 0. В противном случае "Форма реорганизации" должна быть равна 0 </usch:error>
																		</sch:assert>
																		<sch:assert test=" (count(@КПП)!=0 and @ФормРеорг!=0) or (count(@КПП)=0 and @ФормРеорг=0)  ">
																			<usch:error code="0400300003" path="@КПП" value-of-select="@ФормРеорг">	Если заполнено поле "КПП реорганизованной организации", то поле "Форма реорганизации" не должно быть равно 0. В противном случае "Форма реорганизации" должна быть равна 0 </usch:error>
																		</sch:assert>
																	</sch:rule>
																</sch:pattern>
															</xs:appinfo>
															<!---*********************************************************************************************************************************-->
															<xs:documentation/>
															<xs:documentation>Сведения о реорганизованной (ликвидированной) организации</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="ФормРеорг" use="required">
																<xs:annotation>
																	<xs:documentation>Код формы реорганизации (ликвидация)</xs:documentation>
																	<xs:documentation>Принимает значение:
0 – ликвидация   | 
1 – преобразование   | 
2 – слияние   | 
3 – разделение   | 
5 – присоединение   | 
6 – разделение с одновременным присоединением</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:length value="1"/>
																		<xs:enumeration value="1"/>
																		<xs:enumeration value="2"/>
																		<xs:enumeration value="3"/>
																		<xs:enumeration value="5"/>
																		<xs:enumeration value="6"/>
																		<xs:enumeration value="0"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="optional">
																<xs:annotation>
																	<xs:documentation>ИНН организации</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="КПП" type="КППТип" use="optional">
																<xs:annotation>
																	<xs:documentation>КПП</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="НаимОрг" use="required">
													<xs:annotation>
														<xs:documentation>Наименование организации</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="required">
													<xs:annotation>
														<xs:documentation>ИНН организации</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="КПП" type="КППТип" use="required">
													<xs:annotation>
														<xs:documentation>КПП</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="НПФЛ">
											<xs:annotation>
												<xs:documentation/>
												<xs:documentation>Налогоплательщик - физическое лицо</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ФИО" type="ФИОТип">
														<xs:annotation>
															<xs:documentation/>
															<xs:documentation>Фамилия, имя, отчество физического лица</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="ИННФЛ" type="ИННФЛТип" use="required">
													<xs:annotation>
														<xs:documentation>ИНН физического лица</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:choice>
									<xs:attribute name="ОКВЭД" type="ОКВЭДТип" use="required">
										<xs:annotation>
											<xs:documentation>Код вида экономической деятельности по классификатору ОКВЭД</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Тлф" use="optional">
										<xs:annotation>
											<xs:documentation>Номер контактного телефона</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Подписант">
								<xs:annotation>
									<!---*********************************************************************************************************************************************************-->
									<xs:appinfo>
										<sch:pattern name="Присутствие сведений о представителе">
											<sch:rule context="Подписант">
												<!-- 19. Контроль  обязательности  присутствия  Сведений  о  представителе  налогоплательщика  в  зависимости  от  значения  ПрПодп  -->
												<sch:assert test="usch:iif(@ПрПодп=2, count(СвПред)!=0, (count(СвПред)!=0 or count(СвПред)=0))">
													<usch:error code="0400300003" path="@ПрПодп" value-of-select="@ПрПодп"> При значении поля "Признак налогоплательщика" = 2 должны быть заполнены сведения о представителе </usch:error>
												</sch:assert>
											</sch:rule>
										</sch:pattern>
									</xs:appinfo>
									<!---*********************************************************************************************************************************************************-->
									<xs:documentation/>
									<xs:documentation>Сведения о лице, подписавшем документ</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ФИО" type="ФИОТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation/>
												<xs:documentation>Фамилия, имя, отчество</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПред" minOccurs="0">
											<xs:annotation>
												<xs:documentation/>
												<xs:documentation>Сведения о представителе налогоплательщика, налогового агента</xs:documentation>
												<xs:documentation><![CDATA[Элемент обязателен при <ПрПодп>=2]]></xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="НаимДок" use="required">
													<xs:annotation>
														<xs:documentation>Наименование документа, подтверждающего полномочия представителя</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="120"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НаимОрг" use="optional">
													<xs:annotation>
														<xs:documentation>Наименование организации - представителя налогоплательщика, налогового агента</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ПрПодп" use="required">
										<xs:annotation>
											<xs:documentation>Признак лица, подписавшего документ</xs:documentation>
											<xs:documentation>Принимает значение:
1 – налогоплательщик, налоговый агент   |
2 – представитель налогоплательщика, налогового агента</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="НДС">
								<xs:annotation>
									<xs:documentation/>
									<xs:documentation>Налоговая декларация по налогу на добавленную стоимость</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СумУплНП">
											<xs:annotation>
												<!---**********************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие элемента ОКТМО в зависимости от значения элемента ПоМесту">
														<sch:rule context="СумУплНП">
															<!-- 20. Контроль  обязательности  присутствия элемента ОКТМО в зависимости от значения элемента ПоМесту -->
															<sch:assert test="usch:iif(   (../../@ПоМесту)=231, (count(@ОКТМО)=0 or count(@ОКТМО)!=0) , count(@ОКТМО)!=0  )">
																<usch:error code="0400300003" path="@ОКТМО" value-of-select="count(@ОКТМО)">  При значении поля "По месту" на Титульном листе не равном 231, поле ОКТМО должно быть заполнено </usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---**********************************************************************************************************************************************-->
												<!---**********************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие элемента КБК в зависимости от значения элемента ПоМесту">
														<sch:rule context="СумУплНП">
															<!-- 21. Контроль  обязательности  присутствия элемента КБК в зависимости от значения элемента ПоМесту -->
															<sch:assert test="usch:iif(   (../../@ПоМесту)=231, (count(@КБК)=0 or count(@КБК)!=0) , count(@КБК)!=0  )">
																<usch:error code="0400300003" path="@КБК" value-of-select="count(@КБК)">   При значении поля "По месту" на Титульном листе не равном 231, поле КБК должно быть заполнено </usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---**********************************************************************************************************************************************-->
												<!---**********************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие элемента КБК в зависимости от наличия элемента ОКТМО">
														<sch:rule context="СумУплНП">
															<!-- 22. Контроль  обязательности  присутствия элемента КБК в зависимости от наличия элемента ОКТМО -->
															<sch:assert test="usch:iif(   count(@ОКТМО)!=0, count(@КБК)!=0 , (count(@КБК)=0 or count(@КБК)!=0)  )">
																<usch:error code="0400300001" path="@КБК" value-of-select="count(@КБК)">  Если заполнено поле ОКТМО, то должно быть заполнено и поле КБК </usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---**********************************************************************************************************************************************-->
												<!---**********************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие НомДогИТ, ДатаНачДогИТ, ДатаКонДогИТ     ">
														<sch:rule context="СумУплНП">
															<!-- 23. Контроль  обязательности  присутствия  НомДогИТ в  зависимости  от  значения  КПП -->
															<sch:assert test="usch:iif(  (../../@ПоМесту)=227 , count(@НомДогИТ)!=0, count(@НомДогИТ)=0 )">
																<usch:error code="0400300003" path="@НомДогИТ" value-of-select="count(@НомДогИТ)"> Если "По месту" на Титульном листе равно 227, то строка 060 должна быть заполнена. В противном случае строка 060 должна быть пустой </usch:error>
															</sch:assert>
															<!-- 24. Контроль  обязательности  присутствия  ДатаНачДогИТ в  зависимости  от  значения  КПП -->
															<sch:assert test="usch:iif(   (../../@ПоМесту)=227 , count(@ДатаНачДогИТ)!=0, count(@ДатаНачДогИТ)=0 )">
																<usch:error code="0400300003" path="@ДатаНачДогИТ" value-of-select="count(@ДатаНачДогИТ)"> Если "По месту" на Титульном листе равно 227, то строка 070 должна быть заполнена. В противном случае строка 070 должна быть пустой </usch:error>
															</sch:assert>
															<!-- 25. Контроль  возможности  присутствия  ДатаКонДогИТ в  зависимости  от  значения  КПП -->
															<sch:assert test="usch:iif(   (../../@ПоМесту)=227 , (count(@ДатаКонДогИТ)!=0 or count(@ДатаКонДогИТ)=0) , count(@ДатаКонДогИТ)=0)">
																<usch:error code="0400300003" path="@ДатаКонДогИТ" value-of-select="count(@ДатаКонДогИТ)"> Если "По месту" на Титульном листе равно 227, то строка 080 должна быть заполнена. В противном случае строка 080 должна быть пустой </usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---**********************************************************************************************************************************************-->
												<!---***********************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие одного из элементов  СумПУ_173.5  или  СумПУ_173.1  в зависимости от наличия элемента ОКТМО">
														<sch:rule context="СумУплНП">
															<!-- 26. Контроль  обязательности  присутствия одного из элементов  СумПУ_173.5  или  СумПУ_173.1  в зависимости от наличия элемента ОКТМО -->
															<sch:assert test="usch:iif(   count(@ОКТМО)!=0 , 
									count(@СумПУ_173.5)!=0 or count(@СумПУ_173.1)!=0  ,  
									count(@СумПУ_173.5)=0 and count(@СумПУ_173.1)=0     )">
																<usch:error code="0400300001" path="НДС/СумУплНП/@ОКТМО" value-of-select="count(НДС/СумУплНП/@ОКТМО)"> Если заполнено поле ОКТМО, то должна быть заполнена хотя бы одна из строк 030, 040, 050. Если поле ОКТМО не заполнено, то все эти строки должны быть пустыми </usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---***********************************************************************************************************************************************-->
												<xs:documentation>Раздел 1</xs:documentation>
												<xs:documentation>Сумма налога, подлежащая уплате в бюджет (возмещению из бюджета), по данным налогоплательщика</xs:documentation>
												<xs:documentation>part</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="ОКТМО" type="ОКТМОТип" use="optional">
													<xs:annotation>
														<xs:documentation>строка 010</xs:documentation>
														<xs:documentation>Код по ОКТМО</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="КБК" type="КБКТип" use="optional">
													<xs:annotation>
														<xs:documentation>строка 020</xs:documentation>
														<xs:documentation>Код бюджетной классификации</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="СумПУ_173.5" use="optional">
													<xs:annotation>
														<xs:documentation>строка 030</xs:documentation>
														<xs:documentation>Сумма налога, подлежащая уплате в бюджет в соответствии с пунктом 5 статьи 173 Налогового кодекса Российской Федерации</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:integer">
															<xs:totalDigits value="14"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="СумПУ_173.1" use="optional">
													<xs:annotation>
														<xs:documentation>строка 040 или строка 050</xs:documentation>
														<xs:documentation>Сумма налога, подлежащая уплате в бюджет в соответствии с пунктом 1 статьи 173 Налогового кодекса Российской Федерации / Сумма налога, исчисленная к возмещению из бюджета в соответствии с пунктом 2 статьи 173 Налогового кодекса Российской Федерации</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:integer">
															<xs:totalDigits value="14"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НомДогИТ" use="optional">
													<xs:annotation>
														<xs:documentation>строка 060</xs:documentation>
														<xs:documentation>Регистрационный номер договора инвестиционного товарищества</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
